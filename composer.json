{
    "name": "sdkcodes/lara-paystack",
    "description": "A simple package for using paystack with laravel",
    "type": "laravel-package",
    "keywords": ["laravel paystack", "laravel", "paystack", "laravel payment", "paystack laravel", "laravel-5-package"],
    "require": {
        "laravel/framework": ">=5.4",
        "illuminate/support": ">=5.4",
        "guzzlehttp/guzzle": "~6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sdkcodes",
            "email": "sdkcodes@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sdkcodes\\LaraPaystack\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sdkcodes\\LaraPaystack\\LaraPaystackServiceProvider"
            ]
        }
    }
}
